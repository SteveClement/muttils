# $Id$
# rcfile example for Python muttils package
# RCPATH: /etc/muttilsrc:/usr/local/etc/muttilsrc:~/.muttilsrc

[browser]
textbrowser = w3m
xbrowser = firefox
homepage = http://localhost
# defaults:
# textbrowser =
# xbrowser =
# homepage =

[messages]
mailer = /usr/local/bin/mutt
maildirs = ~/Mail, ~/mail-archive
# defaults:
# mailer = mail
# maildirs = $MAIL
# or
# maildirs = ~/Mail
# or
# maildirs = ~/Maildir
