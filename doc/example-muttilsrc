# $Id$
# rcfile example for Python muttils package
# RCPATH: /etc/muttilsrc:/usr/local/etc/muttilsrc:~/.muttilsrc

[browser]
textbrowser = w3m
xbrowser = firefox
homepage = http://localhost
# defaults:
# textbrowser =
# xbrowser =
# homepage =

[messages]
mailer = /usr/local/bin/mutt
maildirs = ~/Mail, ~/mail-archive
# defaults:
# mailer = mail
# maildirs = $MAIL
# or
# maildirs = ~/Mail
# or
# maildirs = ~/Maildir

[can]
cpan = ftp://ftp.rz.ruhr-uni-bochum.de/pub/CPAN
CTAN = ftp://ftp.dante.de/tex-archive
# defaults:
# cpan = ftp://ftp.cpan.org/pub/CPAN
# ctan = ftp://ftp.ctan.org/tex-archive
